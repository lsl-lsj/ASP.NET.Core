@page
<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>用户列表</title>
    </head>
    <body>
        <div>
            <table>
                <thead>
                    <tr>
                        <th>用户名</th>
                        <th>ID</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        for(int i = 1; i < 10; i++)
                        {
                            <tr>
                                <td>@string.Format("用户 {0}", i)</td>
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </body>
</html>